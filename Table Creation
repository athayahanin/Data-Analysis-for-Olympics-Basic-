CREATE TABLE Olahraga 
(
ID_Sport NVARCHAR(20) NOT NULL, 
Tanggal DATE(20) NOT NULL, 
Cabang_Olahraga NVARCHAR(20) NOT NULL, 
ID_Atlet NVARCHAR(20) NOT NULL,
);

