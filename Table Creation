(‘S005’,’2021-08-06’,’Badminton’,’A003’);

CREATE TABLE Atlet
(
ID_Atlet NVARCHAR(20) NOT NULL,
Nama_Atlet NVARCHAR(20) NOT NULL,
Kewarganegaraan NVARCHAR(20) NOT NULL
);

